@inject ISearchProduct SearchProduct
@if (this.Product != null)
{




    <tr>
        <NavLink href="@($"/product/{Product.Id}")">
            <td>@Product.Name</td>
        </NavLink>
        <td>@Product.Brand</td>
        <td>@Product.Price.ToString("c")</td>
    </tr>
}

@code {

    [Parameter]
    public Product Product { get; set; }
    

}
